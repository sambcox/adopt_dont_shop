<h1>New Shelter</h1>
<%= form_with url: '/shelters', local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :zip_code %>
  <%= f.number_field :zip_code %>

  <%= f.label :foster_program %>
  <%= f.check_box :foster_program %>

  <%= f.label :rank %>
  <%= f.number_field :rank %>

  <%= f.submit %>
<% end %>
